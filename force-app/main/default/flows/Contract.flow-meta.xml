    <formulas>
        <name>forACV_USD</name>
        <dataType>Currency</dataType>
        <expression>{!Loop_Related_Assets.Total_Price__c} * {!Loop_Related_Assets.Exchange_Rate__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>forARR_USD</name>
        <dataType>Currency</dataType>
        <expression>{!Loop_Related_Assets.ARR__c} * {!Loop_Related_Assets.Exchange_Rate__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>forAssetHasStarted</name>
        <dataType>Number</dataType>
        <expression>IF({!Loop_Related_Assets.Start_Date__c} &lt;= TODAY(), 1, 0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>forC_iARR</name>
        <dataType>Currency</dataType>
        <expression>IF(ISNULL({!$Record.Previous_ARR__c}), {!$Record.ARR__c}, {!$Record.ARR__c} - {!$Record.Previous_ARR__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>forC_MRR</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.ARR__c} / 12</expression>
        <scale>2</scale>
    </formulas>
