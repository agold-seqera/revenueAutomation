<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARR_USD__c</fullName>
    <label>ARR (USD)</label>
    <type>Text</type>
    <formula>IF(ISBLANK(Exchange_Rate__c),
NULL,
"$" + TEXT(ROUND(ARR__c / Exchange_Rate__c, 2))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <trackHistory>false</trackHistory>
    <description>Annual Recurring Revenue converted to USD using division formula with historical exchange rate</description>
</CustomField>
